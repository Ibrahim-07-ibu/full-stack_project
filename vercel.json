{
  "version": 2,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/health",
      "destination": "/api/index.py"
    },
    {
      "source": "/(css|js|html)/(.*)",
      "destination": "/Frontend/$1/$2"
    },
    {
      "source": "/(.*).html",
      "destination": "/Frontend/$1.html"
    },
    {
      "source": "/(.*)",
      "destination": "/Frontend/index.html"
    }
  ]
}