{
  "version": 2,
  "functions": {
    "api/*.py": {
      "maxDuration": 15
    }
  },
  "rewrites": [
    {
      "source": "/api/test",
      "destination": "/api/test.py"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/api",
      "destination": "/api/index.py"
    }
  ]
}