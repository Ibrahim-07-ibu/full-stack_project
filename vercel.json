{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/health",
      "dest": "/api/index.py"
    },
    {
      "src": "/Frontend/(.*)",
      "dest": "/Frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/$1"
    }
  ]
}