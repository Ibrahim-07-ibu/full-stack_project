{
  "version": 2,
  "env": {
    "ENVIRONMENT": "production",
    "PYTHONUNBUFFERED": "1"
  },
  "functions": {
    "api/index.py": {
      "runtime": "vercel-python@0.1.1"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ]
    },
    {
      "src": "/api",
      "dest": "/api/index.py",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ]
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}